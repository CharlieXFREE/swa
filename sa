local EmojiSecureV2 = {
    Encode = function(data)
        local Output = ""
        for i,v in next, data:split("") do 
            if Characters[v] then 
                Output = Output.."âœ…"..Characters[v] 
            elseif v == [[\]] then 
                Output = Output.."âœ…"..Characters["backslash"]
            elseif v == [[`]] then 
                Output = Output.."âœ…"..Characters["grave"]
            end
        end
        return Output
    end,
    Decode = function(data)
        local Output = ""
        for i,v in next, data:split("âœ…") do 
            for k,x in next, Characters do 
                if x == v then 
                    if k ~= "backslash" and k ~= "grave" then
                        Output = Output..k 
                    else 
                        if k == "backslash" then 
                            Output = Output..[[\]] 
                        elseif k == "grave" then 
                            Output = Output..[[`]] 
                        end 
                    end
                end
            end
        end
        return Output
    end
}

return EmojiSecureV2
